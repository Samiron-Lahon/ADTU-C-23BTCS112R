/*Program to find the Fibonacci series*/

#include <stdio.h>

int main() {
    int n;
    printf("Enter a number:\n");
    scanf("%d", &n);

    if(n==0)
    {
        printf("Factorial of 0 is 1");
    }

    int fac = 1;  

    for (int i = 1; i <= n; i++) {
        fac = fac * i;
    }

    printf("Factorial will be: %d\n", fac);

    return 0;
}
